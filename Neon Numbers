# java_programs
java programmings
#The given program is to check whether the given program is a neon number or not
import java.util.*;
public class Main
{
	public static void main(String[] args) 
	{
	    Scanner sc=new Scanner(System.in);
	    int n=sc.nextInt();
	    int sq=n*n;
	    int r,s=0;
	    while(sq>0)
	    {
	        r=sq%10;
	        s=s+r;
	        sq=sq/10;
	    }
	    if(s == n)
	    {
	        System.out.println(n +" = "+"Neon Number");
	    }
	    else
	    {
	        System.out.println(n +" = "+" Not a Neon Number");
	    }
	}
}
